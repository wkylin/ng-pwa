  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "http:server": "cd dist && http-server -p 9088",
    "ngrok": "ngrok http 9088",
    "copy:pem": "copy pem dist",
    "rm:dist": "rimraf dist",
    "mkdir": "make-dir dir",
    "ngsw:server": "cd dist && http-server -S  -C cert.pem -K key.pem",
    "ngh:build": "ng build --prod --base-href \"https://wkylin.github.io/ng-pwa/\"",
    "ngh": "ngh",
    "ngh:page": "concurrently \"npm run ngh:build\" \"npm run ngh\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
